{
  "compilerOptions": {
    "module": "Node16",
    "esModuleInterop": true,
    "target": "ES6",
    "moduleResolution": "Node16",
    "outDir": "dist",
    "rootDir": "src",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "useUnknownInCatchVariables": false,
  },
  "ts-node": {
      "esm": true
  },
  "exclude": [
    "node_modules",
    "public_html",
    "test",
    "bin",
    "lib"
 ]
}
